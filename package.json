{
  "name": "image-gallery",
  "version": "1.0.0",
  "description": "made  with react, tailwindcss & pixabay api",
  "keywords": [],
  "main": "src/index.js",
  "dependencies": {
    "autoprefixer": "9.7.6",
    "postcss-cli": "7.1.1",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-scripts": "3.0.1",
    "tailwindcss": "1.4.5"
  },
  "devDependencies": {
    "typescript": "3.8.3"
  },
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build:css": "postcss src/assets/tailwind.css -o src/assets/main.css",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}